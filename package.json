{
  "name": "visioncare",
  "version": "1.0.0",
  "private": true,
  "description": "VisionCare - AI-Powered Eye Health Assistant",
  "workspaces": [
    "backend",
    "mobile"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:mobile\"",
    "dev:backend": "npm run dev --workspace=backend",
    "dev:mobile": "npm run start --workspace=mobile",
    "build": "npm run build --workspace=backend",
    "test": "npm run test --workspaces"
  },
  "keywords": [
    "eye-health",
    "vision-care",
    "ai",
    "react-native",
    "expo"
  ],
  "author": "VisionCare Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
